{
  "version": 2,
  "builds": [{ "src": "server.js", "use": "@vercel/node" }],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js",
      "methods": ["GET", "POST"],
      "headers": {
        "Access-Control_Allow-Origin": "process.env.FRONTEND_URL"
      }
    }
  ]
}
